init_config:

instances:
  [{}]

logs:
<%- @logs.each do | log | -%>
  - type: file
    path: <%= log['path'] %>
    service: <%= log['service'] %>
    source: <%= log['source'] %>
<%- if log.key?('pattern') %>
    log_processing_rules:
      - type: multi_line
        name: new_log_start
        pattern: <%= log['pattern'] %>
<% end -%>
<% end -%>
  
