init_config:

instances:
  [{}]

logs:
<%- @logs.each do | log | -%>
  - type: file
    path: <%= log['log_location'] %>
    service: <%= actual_service %>
    source: <%= log['log_dest'] %>
<%- if log['pattern'] -%>
    log_processing_rules:
      - type: multi_line
        name: new_log_start
        pattern: <%= log['pattern'] %>
<% end %>
<% end %>
  
