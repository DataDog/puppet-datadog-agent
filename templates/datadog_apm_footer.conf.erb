<% if not @apm_env.empty? -%>
[trace.agent]
env: <%= @apm_env %>
<% end -%>

<% if @apm_analyzed_spans -%>
[trace.analyzed_spans]
<% @apm_analyzed_spans.each do |span, value| -%>
<%= span %>: <%= value %>
<% end %>
<% end -%>

<% if @apm_automatic_scrubbing -%>
[trace.obfuscation]
<% @apm_automatic_scrubbing.each do |service, data| -%>
<%= service %>:
  <%= data %>
<% end %>
<% end -%>