init_config:

instances:
    -   url: <%= @url %>
        port: <%= @port %>
<% if @tags and ! @tags.empty? -%>
        tags:
  <%- Array(@tags).each do |tag| -%>
    <%- if tag != '' -%>
            - <%= tag %>
    <%- end -%>
  <%- end -%>
<% end -%>

options:
  items: <%= @items %>
  slabs: <%= @slabs %>
